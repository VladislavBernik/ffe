<% let text = `
<p>
  Lorem ipsum dolor sit amet consectetur adipisicing elit. Eius eveniet quae
  quia aspernatur quos, harum eos placeat officiis, tempore nihil beatae eaque.
  Quidem dolorum aliquid earum quasi iure, voluptate ipsum.
</p>
` %> <% let menu = [ { title: "Моя страница", url: "/" }, { title: "Соседняя страница", url: "/second-page" }] %>

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/main.css" type="text/css" />
    <title><%= title %></title>
  </head>
  <body>
    <header>
      <nav>
        <% menu.forEach(item => { %>
        <a href="<%= item.url %> "><%= item.title %> </a>
        <%}) %>
      </nav>
    </header>
    <h1><%= title %></h1>
    <div><%- text %></div>
  </body>
</html>
